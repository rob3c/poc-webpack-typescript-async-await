{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "async-await-hello-world",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/bin/async-await-hello-world/app.bundle.js",
            "stopOnEntry": false,
            "args": [
            ],
            "cwd": "${workspaceRoot}/bin/async-await-hello-world",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/bin/async-await-hello-world"
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outDir": null,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        }
    ]
}
